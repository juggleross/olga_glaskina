<% content_for :body_class, "section-exhibitions page-exhibitions site-responsive responsive-top-size-1023 responsive-nav-standard responsive-nav-side-position-left analytics-track-all-links page-param-overview page-param-127 page-param-type-section page-param-type-exhibition_id nopopups"%>
<% content_for :title, "Projects | "%>
<div id="main_content" class="clearwithin">
  <!--contentstart-->
  <div class="exhibition">
    <div class="subsection-exhibition-detail-page subsection-wrapper-overview">
      <% glob_name = @home_project.first.photo_type%>
      <div class="exhibition-header heading_wrapper clearwithin">
        <h1 data-onclick-url="#"><span class="h1_heading">'<%=@home_project.first.title %>'</span><span class="separator">: </span> <span class="h1_subtitle"><%=glob_name%></span></h1>
        <div id="exhibition-status-current-forthcoming" class="hidden">
          Current
        </div>
        <div class="date">
          <%=@home_project.first.date%>
        </div>
        <div id="sub_nav" class="navigation  noprint clearafter clearwithin">
          <div class="subnav-reveal-wrapper">
            <div class="subnav_active_indicator"></div>
            <div id="custom_sub_nav_reveal"></div>
          </div>
          <ul>
            <li id="sub-item-overview" class="active first scrollto">
              <a href="#content_module">Overview</a>
            </li>
            <li class="sub-item-share">
            </li>
          </ul>
        </div>
      </div>
      <div id="content" class="content_alt clearwithin">
        <div class="subsection-works">
          <div class="artworks-slideshow">
            <div class="slick-artworks" style="width:100%; margin: 0; padding: 0;" >
              <% @artworks.each do |artwork| %>
                <div class="slick-item" data-caption="<span class=&#34;artist&#34;><strong><%=glob_name %></strong></span>, <span class=&#34;title&#34;><em><%= artwork.photo_name %></em></span>, © <%=glob_name %> Archive" style="margin:0 16px;">
                <a href="<%= artwork.image_cloud %>" class="slick_image_popup" data-slick-index="" data-fancybox-title="<div class=&#34;artist&#34;><span class=&#34;artist&#34;><strong><%= artwork.photo_name%></strong></span></div>
                <div class=&#34;copyright&#34;>© <%=glob_name%> Archive</div>
                " data-zoomimage="<%= artwork.image_cloud %>" rel="group">
                <img src="/assets/shim.png" class="slideshow-img " data-shop_button="" data-lazy="<%= artwork.image_cloud %>" data-artwork_id="<%=artwork.artwork_id %>" data-artwork_path="artworks_standalone/<%=artwork.artwork_id %>" data-imgwidth="2000" data-imgheight="1398" alt="<div class=&#34;artist&#34;><span class=&#34;artist&#34;><strong><%=glob_name %></strong></span></div>
              "/>
            </a>
            <div class="slideshade"></div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div class="inner slick-caption-wrapper">
    <div class="slick-caption-inner clearwithin">
      <div class="slick-artwork-caption">
        <span></span>
      </div>
      <div class="slick-artwork-enlarge">
        <a class="slick_image_popup">Enlarge</a>
      </div>
      <div class="artwork-buy-button"><a href="#"></a></div>
    </div>
  </div>
</div>
</div>
</div>
</div>